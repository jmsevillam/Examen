   "source": [
    "import numpy as np\n",
    "import matplotlib.pylab as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "m=1.3\n",
    "b=1.58\n",
    "def line(x):\n",
    "    return m*x+b\n",
    "\n",
    "x=np.linspace(0,10,100)\n",
    "noise=np.random.rand(len(x))\n",
    "y=line(x)+noise"
   ]
  },
